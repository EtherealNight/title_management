			//Order Statust List Dataprovider
			[Bindable]
			private var _orderStatusDP;
			
			//Order Status Result Event. Populates the Order Status Dropdown filter with available order types. 
			private function StatusList_ResultEvent(e:ResultEvent):void{
				_orderStatusDP = e.result;
			}
			//Order data Result Event. Populates the Order Management tab with order data.
			private function OrderData_ResultEvent(evt:ResultEvent):void{
			        _orderDataDP = evt.result;
			}
			
			//Call the project management module loader
			private function callProjectManagementWindow():void{
				//variable newProjectManagementWindow types as instance of project Management Module. 
				var newProjectManagementWindow:projectManagementLoader = 
					PopUpManager.createPopUp(this,projectManagementLoader, true) as projectManagementLoader;
					PopUpManager.centerPopUp(newProjectManagementWindow);
			}
			
			//call the suspend employee popup window
			private function callSuspendEmployeeProfile():void{
			//variable newSuspendEmployeeProfile typed as instance of suspend employee account component
				var newSuspendEmployeeProfile:suspendEmployeeProfile;
			}
			
			//~~~~~~~~~~~~~~~~~Login System Block ~~~~~~~~~~~~~~~~~~//
			private function loginUser(event:KeyboardEvent):void {
				if (event.keyCode == Keyboard.ENTER)
				loginUserRO.createSession(fm_userName.text, fm_Password.text);
			}
			
			private function loginUserOnClick(event:MouseEvent):void{
				loginUserRO.createSession(fm_userName.text, fm_Password.text);
			}
			
			//log out user
			private function logoutUser():void{
				this.sessionDP.LOGGEDIN = '0'
				currentState = 'login';
				
				
			}
			
			//login data provider
			private var sessionDP;
			
			//client side session variable
			private var _sessionStatus;
			
			private function set sessionStatus(_sessionStatus):void{
				_sessionStatus = '0';
			}
			
			//login result handler
			private function login_ResultHandler(evt:ResultEvent):void{
				sessionDP = evt.result;
				if (sessionDP.LOGGEDIN == '1') {
					currentState = 'managers';
					_sessionStatus = '1';
					
				}
				else 
				{ Alert.show("We are unable to log you in at this time. Please contact application administrator.");
				
				}	
				
			}
			
			//Alert user variable
			private var notifyUser;
			
			//timer 
			private var aTimer:Timer;
			//timer function
			private function timerFunc():void{
				aTimer = new Timer(30000,1);
			}
			//session timeout warning method
			private  function preTimeOut():void{
				//alert user of impending session time out. 
				notifyUser = Alert.show("You have been idle for" );
				
			}
			
			//Contact administrator to register user.
			private function contactAdmin():void{
				Alert.show("Please email Johnnie Reece to get setup in the company portal.", 'Contact Site Admin', mx.controls.Alert.OK);
			}
			
			//clear login form on login
			private function clearLogin():void{
				fm_userName.text = '';
				fm_Password.text = '';
			}
			//~~~~~~~~~~~~~~~~ onBoarding Tab methods. ~~~~~~~~~~~~~~~~~~~//
			
		
			//call the add new employee popup window
			private function callCreateNewUser():void{
				// variable newCreateNewUser typed as instance of new user creation component.
				var newCreateNewUserWindow:createNewUser = 
					PopUpManager.createPopUp(this,createNewUser, true) as createNewUser;
				PopUpManager.centerPopUp(newCreateNewUserWindow);
			}
			//call the remove user rights component
			private function callRevokeRights():void{
				var newRevokeRightsComp:suspendEmployeeProfile =
					PopUpManager.createPopUp(this, suspendEmployeeProfile, true) as suspendEmployeeProfile;
					PopUpManager.centerPopUp(newRevokeRightsComp)
			}
			//~~~~~~~~~~~~~~~~~ Report Tab methods. ~~~~~~~~~~~~~~~~~~~//
			//Reports Tab Menubar function.
	       //Tied to states in the application. 
			private function clickNav(event:MenuEvent):void{
			
				if(event.item.@label=='Login')
				currentState = 'login';
				
				else if (event.item.@label=='Logout')
					currentState = 'loggedOut';
				
			}
			//~~~~~~~~~~~~~~~Onboarding Tab Methods ~~~~~~~~~~~~~~~~//
			//Populate new hire display
			private function newHires():void{
				//remote object call
				
			}
