			[Bindable]
			private var recentHires;
			
			//Populate new hire ResultHandler
			private function newHire_ResultHandler(evt:ResultEvent):void{
			 //Assign DP variable to the result event
				recentHires = evt.result ;
			}
			
			//Processing State Methods
			
			
			//Stats Dashboard methods
			//Launch the stats dashboard for the user signed in. 
		    //Pass in the user's role. Role EQ userDashboard to initialize.
			private function launchStatsDash():void{
				
			}
			
			//Search file functionality 
			private function findFile():void{
				//Remote object call to search DB for file
				getDataRO.findFile(searchBox.text);
			}
			
			//file search DP Variable
			private var fileSearchResult;
			
			//Find File Result event
			private function findFile_ResultHandler(e:ResultEvent):void{
				
				//set the result event to the result variable
				fileSearchResult = e.result;
				
				//Evaluate  the returned response to determine whether to open the order search result window or return simple alert.
				if (fileSearchResult.VOID == '1') {
				//Launch the search window
					var newFindFileWin:findFileComp =
					PopUpManager.createPopUp(this, findFileComp, true) as
					findFileComp;
					newFindFileWin.fileGridDP = e.result;
					
				// variable newCreateNewUser typed as instance of new user creation component.
				var newStatsWindow:statsDashboard = 
					PopUpManager.createPopUp(this,statsDashboard, true) as statsDashboard;
				PopUpManager.centerPopUp(newStatsWindow); }
				else { 
					Alert.show("No order matching that order number was found. Please try again.",'Order Alert'); }
			}
			
			//~~~~~~~~~~ Stats Panel~~~~~~~~~//
			//My Stats Method
			private function openMyStats(event:MouseEvent):void{
			//Open the my stats panel
				var newMyStats = PopUpManager.createPopUp(this, myStats, true) as myStats;
				
			}
			//Launch Statistics Module
			private function launchStatsModule(event:MouseEvent):void{
				//cast new instance of the stats module
				var newStatsWin:statsDashboard = PopUpManager.createPopUp(this,statsDashboard, true) as statsDashboard;
				PopUpManager.centerPopUp(newStatsWin);
				
			}
			
			
		]]>
